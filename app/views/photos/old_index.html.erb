  <div class="card" style="width: 30rem; margin: 0 auto; float: none; margin-botom: 10px">
      <table class="table">
        <thead>
          <tr>
            <th>Image</th>
            <th>Caption</th>
            <th colspan="3"></th>
          </tr>
        </thead>

        <tbody>
            <tr>
              <%= @photos.reverse.each do |photo| %>
              <td><%= image_tag photo.image_url %></td>
              <td><%= photo.user.username %></td>
              <td><%= photo.caption %></td>
      <!-- voteable -->
                <td> <button type="button" class="btn btn-primary"> <%= link_to like_photo_path(photo), method: :put do %>
                  Upvote
                  <%= photo.get_upvotes.size %>
                <% end %> </button> </td>
                <td> <button type="button" class="btn btn-primary"><%= link_to dislike_photo_path(photo), method: :put do %>
                  Downvote
                  <%= photo.get_downvotes.size %>
                <% end %> </button></td>
      <!-- voteable -->
              <td><%= link_to 'Show', photo %></td>
              <td><%= link_to 'Edit', edit_photo_path(photo) %></td>
              <td><%= link_to 'Destroy', photo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
              <td>
            </tr>
        </tbody>
      </table>
      <%= link_to 'New Photo', new_photo_path %>
      <%= @photos.inspect %>
    <% end %>
 </div>
</div>
